{
  "name": "umich-lib-ui",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist --force-local",
    "build": "lerna exec --parallel -- babel src -d lib --config-file ../../babel.config.js",
    "build:watch": "lerna exec --parallel -- babel src -d lib --config-file ../../babel.config.js -w",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "start": "npm install && npm run bootstrap && npm run build:watch & npm run storybook & (cd www/ && npm install && npm run develop)",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "build:storybook": "build-storybook -c .storybook -o dist",
    "publish": "lerna publish",
    "sketch": "story2sketch"
  },
  "version": "0.0.0",
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-storyshots": "^5.0.5",
    "@storybook/react": "next",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.7",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "jest": "^24.5.0",
    "lerna": "^3.13.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.5",
    "story2sketch": "^1.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
